{
  "name": "svgmoji",
  "version": "2.0.0",
  "description": "Consume svg sprites from popular open source emoji libraries.",
  "keywords": [],
  "homepage": "https://github.com/svgmoji/svgmoji/tree/HEAD/packages/svgmoji",
  "repository": "https://github.com/svgmoji/svgmoji/tree/HEAD/packages/svgmoji",
  "license": "MIT",
  "contributors": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/svgmoji.esm.js",
      "require": "./dist/svgmoji.cjs.js",
      "browser": "./dist/svgmoji.browser.esm.js",
      "types": "./dist/svgmoji.cjs.d.ts",
      "default": "./dist/svgmoji.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "dist/svgmoji.cjs.js",
  "module": "dist/svgmoji.esm.js",
  "browser": {
    "./dist/svgmoji.cjs.js": "./dist/svgmoji.browser.cjs.js",
    "./dist/svgmoji.esm.js": "./dist/svgmoji.browser.esm.js"
  },
  "types": "dist/svgmoji.cjs.d.ts",
  "files": [
    "emoji.json",
    "emoji.min.json",
    "emoticons.json",
    "emoji.json.d.ts",
    "emoji.min.json.d.ts",
    "emoticons.json.d.ts",
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@svgmoji/blob": "2.0.0",
    "@svgmoji/core": "2.0.0",
    "@svgmoji/noto": "2.0.0",
    "@svgmoji/openmoji": "2.0.0",
    "@svgmoji/twemoji": "2.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "meta": {
    "sizeLimit": "10 KB"
  }
}
